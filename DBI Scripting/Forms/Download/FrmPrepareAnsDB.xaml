<Window x:Name="FrmPrepareAnsDB1" x:Class="DBI_Scripting.Forms.Download.FrmPrepareAnsDB"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:wpfTool="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit"
        xmlns:WPF="clr-namespace:Microsoft.TeamFoundation.Controls.WPF;assembly=Microsoft.TeamFoundation.Controls"
        Title="Prepare Ans DB from Server" Height="520.252" Width="768.885" WindowStartupLocation="CenterScreen">
    <Grid Loaded="Grid_Loaded">
        <Label Content="Data file Path :" HorizontalAlignment="Left" Margin="13,59,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.74,-2.112"/>
        <TextBox x:Name="txtDataFileLocation" HorizontalAlignment="Left" Height="23" Margin="103,62,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="551"/>
        <Button x:Name="btnBrowseDataFile" Content="Browse" HorizontalAlignment="Left" Margin="659,62,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="8.079,-5.321" Click="btnBrowseDataFile_Click"/>
        <Button x:Name="btnReject" Content="Reject" HorizontalAlignment="Left" Margin="459,170,0,0" VerticalAlignment="Top" Width="108" Height="31" Click="btnReject_Click"/>
        <Button x:Name="btnDelete" Content="Delete" HorizontalAlignment="Left" Margin="594,170,0,0" VerticalAlignment="Top" Width="108" Height="31" Click="btnDelete_Click"/>
        <Button x:Name="btnRedoReject" Content="Redo Reject" HorizontalAlignment="Left" Margin="459,223,0,0" VerticalAlignment="Top" Width="108" Height="31" Click="btnRedoReject_Click"/>
        <Button x:Name="btnTestToLive" Content="Test to Live" HorizontalAlignment="Left" Margin="594,223,0,0" VerticalAlignment="Top" Width="108" Height="31" Click="btnTestToLive_Click"/>
        <Button x:Name="btnClose" Content="Close" HorizontalAlignment="Left" Margin="532,379,0,0" VerticalAlignment="Top" Width="102" RenderTransformOrigin="8.079,-5.321" Click="btnClose_Click" Height="31"/>
        <Label Content="Project Name :" HorizontalAlignment="Left" Margin="10,26,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.74,-2.112"/>
        <ComboBox x:Name="comProjectName" HorizontalAlignment="Left" Margin="103,29,0,0" VerticalAlignment="Top" Width="166"/>
        <CheckBox x:Name="chkSelectAll" Content="Select All" HorizontalAlignment="Left" Margin="442,458,0,0" VerticalAlignment="Top" Click="chkSelectAll_Click"/>
        <Label Content="WrokSheet" HorizontalAlignment="Left" Margin="66,143,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.74,-2.112"/>
        <Label Content="Respondent Id" HorizontalAlignment="Left" Margin="272,143,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.74,-2.112"/>
        <wpfTool:CheckListBox x:Name="chkListBoxWorksheet" HorizontalAlignment="Left" Height="182" Margin="16,170,0,0" VerticalAlignment="Top" Width="179" ItemSelectionChanged="chkListBoxWorksheet_ItemSelectionChanged"/>
        <wpfTool:CheckListBox x:Name="chkListBoxRespondentId" HorizontalAlignment="Left" Height="303" Margin="216,170,0,0" VerticalAlignment="Top" Width="205" ItemSelectionChanged="chkListBoxRespondentId_ItemSelectionChanged"/>
        <Label x:Name="lblNoOfRejectionId" Content="No of Rejection Id : " HorizontalAlignment="Left" Margin="16,374,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.74,-2.112"/>
        <GroupBox Header="" HorizontalAlignment="Left" Margin="442,274,0,0" VerticalAlignment="Top" Height="78" Width="292">
            <Grid>
                <Label x:Name="lblStatus" Content="Status" HorizontalAlignment="Left" Margin="10,61,0,-31.333" VerticalAlignment="Top" Height="25" Width="263"/>
                <Label x:Name="lblProgress" Content="Progress : " HorizontalAlignment="Left" Margin="10,36,0,-6.333" VerticalAlignment="Top" Height="25" Width="263"/>
            </Grid>
        </GroupBox>
        <Label Content="Ans DB Path :" HorizontalAlignment="Left" Margin="18,92,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.813,-0.45" Width="80"/>
        <TextBox x:Name="txtAnsDBPath" HorizontalAlignment="Left" Height="23" Margin="103,94,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="551"/>
        <Button x:Name="btnBrowseAnsDB" Content="Browse" HorizontalAlignment="Left" Margin="660,95,0,0" VerticalAlignment="Top" Height="22" Width="75" Click="btnBrowse_Click"/>







    </Grid>
</Window>
